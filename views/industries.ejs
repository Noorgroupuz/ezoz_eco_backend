<%- include("parts/header.ejs") %>
<div class="industries_page">
  <div
    class="top-main-page"
    style="background-image: url('../assets/images/industries/background.jpg')"
  >
    <div class="container">
      <h2 class="top-main-page-title"><%= language.industry[lan] %></h2>
    </div>
  </div>
  <div class="industries_page-wrapper">
    <div class="container">
      <div class="row">
        <% industries.forEach(function(item, index){ %>
        <div
        data-wow-delay="<%= index*0.04 %>s"
          data-wow-duration="0.5s"
          class="wow animate__fadeInUp col-lg-3 col-md-4 col-sm-6 col-12 mb-lg-5 mb-4"
        >
          <div class="industry">
            <div class="industry_wrapper-mask"></div>
            <div class="industry_img-wrapper">
              <img
                class="industry_img"
                src="/files/<%= item.industry_image %>"
                alt="img"
              />
            </div>
            <div class="industries_item-content">
              <% if(lan === "uz") { %>
                <h3 class="industry_title"><%= item.industry_title_uz %></h3>
                <% } else { %>
                <h3 class="industry_title"><%= item.industry_title_ru %></h3>
                <% } %>
              <svg
                width="18"
                height="12"
                viewBox="0 0 18 12"
                fill="none"
                xmlns="http://www.w3.org/2000/svg"
              >
                <path
                  d="M1 5.25C0.585786 5.25 0.25 5.58579 0.25 6C0.25 6.41421 0.585786 6.75 1 6.75L1 5.25ZM17.5303 6.53033C17.8232 6.23744 17.8232 5.76256 17.5303 5.46967L12.7574 0.6967C12.4645 0.403807 11.9896 0.403807 11.6967 0.6967C11.4038 0.989593 11.4038 1.46447 11.6967 1.75736L15.9393 6L11.6967 10.2426C11.4038 10.5355 11.4038 11.0104 11.6967 11.3033C11.9896 11.5962 12.4645 11.5962 12.7574 11.3033L17.5303 6.53033ZM1 6.75L17 6.75L17 5.25L1 5.25L1 6.75Z"
                  fill="white"
                />
              </svg>
            </div>
          </div>
        </div>
        <% }) %>
      </div>
    </div>
  </div>
  <section class="how_we_work">
    <div class="container">
      <h2 class="how_we_work-title"><%= language.work[lan] %></h2>
      <img class="how_we_work-image" src="/assets/images/main_page/banner4.jpg" alt="img" />
    </div>
  </section>
  <section class="cars">
    <div class="container">
      <h2 class="cars_title"><%= language.cars.title[lan] %></h2>
      <div class="row">
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/1.png"
              alt="cars"
            />
            <p class="cars_wrapper_name wow animate__bounce"><%= language.cars.cars.car1[lan] %></p>
          </div>
        </div>
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0.2s"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/2.png"
              alt="cars"
            />
            <p class="wow animate__bounce cars_wrapper_name"><%= language.cars.cars.car2[lan] %></p>
          </div>
        </div>
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0.4s"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/3.png"
              alt="cars"
            />
            <p class="wow animate__bounce cars_wrapper_name"><%= language.cars.cars.car3[lan] %></p>
          </div>
        </div>
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0.6s"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/4.png"
              alt="cars"
            />
            <p class="wow animate__bounce cars_wrapper_name"><%= language.cars.cars.car4[lan] %></p>
          </div>
        </div>
      </div>
    </div>
  </section>
