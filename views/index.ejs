    <div class="services_items">
      <div class="swiper services_swiper">
        <div class="swiper-wrapper">
          <% services.forEach(function(item, index){ %>
          <div
            data-wow-delay="<%= index*0.04 %>s"
            data-wow-duration="0.5s"
            class="swiper-slide wow animate__fadeInUp"
          >
            <div class="services_item">
              <a
                class="text-decoration-none"
                href="/service/<%= item.service_id %>"
              >
                <div class="services_item-image-box">
                  <img
                    src="/files/<%= item.service_product_image %>"
                    alt="img"
                    class="services_item-image w-100"
                  />
                </div>
                <div class="services_item-content">
                  <% if(lan === "uz") { %>
                  <h4><%= item.service_title_uz %></h4>
                  <% } else { %>
                  <h4><%= item.service_title_ru %></h4>
                  <% } %>
                </div>
              </a>
            </div>
          </div>
          <% }) %>
        </div>
      </div>
    </div>
  </div>
</section>
<section class="waste">
  <img
    src="/assets/images/main_page/banner2.jpg"
    alt="banner"
    class="w-100 waste_banner"
  />
  <img
    src="/assets/images/main_page/banner2-tablet.jpg"
    alt="banner"
    class="w-100 waste_banner-tablet"
  />
  <img
    src="/assets/images/main_page/banner2-mini.jpg"
    alt="banner"
    class="w-100 waste_banner-mini"
  />
  <div class="container">
    <p><%= language.waste[lan] %></p>
  </div>
</section>
