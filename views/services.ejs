<%- include("parts/header.ejs") %>
<div class="services_page">
  <div
    class="top-main-page"
    style="
      background-image: url('../assets/images/services/main_background.jpg');
    "
  >
    <div class="container">
      <h2 class="top-main-page-title"><%= language.service[lan] %></h2>
    </div>
  </div>
  <div class="services_page-wrapper">
    <div class="container">
      <div class="row">
        <% services.forEach(function(item, index){ %>
        <div
          data-wow-duration="0.5s"
          data-wow-delay="<%= index*0.04 %>s"
          class="wow animate__fadeInUp col-md-4 col-sm-6 col-12 mb-sm-5 mb-4"
        >
          <a
            class="text-decoration-none"
            href="/service/<%= item.service_id %>"
          >
            <div class="services_page-wrapper-card">
              <div class="services_page-wrapper-card-img-box">
                <img
                  class="services_page-wrapper-card-img"
                  src="/files/<%= item.service_product_image %>"
                  alt="img"
                />
              </div>
              <div class="services_page-wrapper-card-content">
                <% if(lan === "uz") { %>
                <p class="services_page-wrapper-card-content-title">
                  <%= item.service_title_uz %>
                </p>
                <% } else { %>
                <p class="services_page-wrapper-card-content-title">
                  <%= item.service_title_ru %>
                </p>
                <% } %>
              </div>
            </div>
          </a>
        </div>
        <% }) %>
      </div>
    </div>
  </div>
  <section class="how_we_work pt-0" style="background-color: transparent">
    <div class="container">
      <h2 class="how_we_work-title"><%= language.work[lan] %></h2>
      <img
        class="how_we_work-image"
        src="/assets/images/main_page/banner4.jpg"
        alt="img"
      />
    </div>
  </section>
  <section class="cars">
    <div class="container">
      <h2 class="cars_title"><%= language.cars.title[lan] %></h2>
      <div class="row">
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/1.png"
              alt="cars"
            />
            <p class="cars_wrapper_name wow animate__bounce">
              <%= language.cars.cars.car1[lan] %>
            </p>
          </div>
        </div>
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0.2s"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/2.png"
              alt="cars"
            />
            <p class="wow animate__bounce cars_wrapper_name">
              <%= language.cars.cars.car2[lan] %>
            </p>
          </div>
        </div>
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0.4s"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/3.png"
              alt="cars"
            />
            <p class="wow animate__bounce cars_wrapper_name">
              <%= language.cars.cars.car3[lan] %>
            </p>
          </div>
        </div>
        <div class="col-lg-3 col-sm-6 mb-sm-0 mb-5">
          <div class="cars_wrapper">
            <img
              data-wow-delay="0.6s"
              data-wow-duration="1s"
              class="cars_wrapper_img wow animate__fadeInLeft"
              src="/assets/images/cars/4.png"
              alt="cars"
            />
            <p class="wow animate__bounce cars_wrapper_name">
              <%= language.cars.cars.car4[lan] %>
            </p>
          </div>
        </div>
      </div>
    </div>
  </section>
